{
  "name": "dia-cura-med",
  "version": "1.0.0",
  "description": "Dia-cure Med is an application that enables patients gain access to a medical doctor in-order to book consultations, study articles, nutritional tips or diets that help improve their health conditions. Also, they have free access to track their intake of drugs and next appointment date. It is a friendly application for both young and old.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "npm install && tsc",
    "start": "node ./dist/server.js",
    "postbuild": "cp -r src/config/openapi.yaml dist/config/",
    "format": "prettier --write \"**/*.{ts,js,json,md,prisma}\"",
    "prettier:check": "prettier --check \"**/*.{ts,js,json,md,prisma}\"",
    "migrate:dev": "npx prisma migrate dev",
    "generate:prisma": "npx prisma generate",
    "swagger": "ts-node src/config/swagger.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harshleey/dia-cura-med.git"
  },
  "keywords": [],
  "author": "Fatima Wonuola Adekunle-logun",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/node": "^24.9.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.62.1",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-jsdoc-swagger": "^1.8.0",
    "express-validator": "^7.2.1",
    "express-xss-sanitizer": "^2.0.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "prisma": "^6.16.3",
    "socket.io": "^4.8.1",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^4.1.12",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.3",
    "@types/serve-static": "^2.2.0",
    "@types/socket.io": "^3.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
