generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id(map: "User_pkey") @default(autoincrement())
  username String @unique(map: "User_username_key")
  email    String @unique(map: "User_email_key")
  password String
  role     Role
}

enum Role {
  PATIENT
  DOCTOR
  ADMIN
}
